{
  "name": "kusokollagen",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yubrot/kusokollagen.git"
  },
  "author": "yubrot",
  "license": "BSD-3-Clause",
  "scripts": {
    "dev": "npm run dev:graphql & npm run dev:next",
    "dev:graphql": "graphql-codegen --watch",
    "dev:next": "next dev",
    "build": "npm run prisma:generate && npm run build:graphql && npm run build:next",
    "build:graphql": "graphql-codegen",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "ladle": "ladle serve",
    "prisma:push": "dotenv -e .env.local -- prisma db push --schema src/backend/prisma/schema.prisma",
    "prisma:generate": "dotenv -e .env.local -- prisma generate --schema src/backend/prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env.local -- prisma studio --schema src/backend/prisma/schema.prisma"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.7.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.6.1",
    "apollo-server-micro": "^3.11.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "next": "^13.0.4",
    "next-auth": "^4.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@graphql-codegen/typescript-resolvers": "^2.7.7",
    "@ladle/react": "^2.4.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "postcss": "^8.4.19",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.3"
  }
}
