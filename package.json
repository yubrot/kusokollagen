{
  "name": "kusokollagen",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yubrot/kusokollagen.git"
  },
  "author": "yubrot",
  "license": "BSD-3-Clause",
  "scripts": {
    "dev": "npm run dev:graphql & npm run dev:next",
    "dev:graphql": "graphql-codegen --watch",
    "dev:next": "next dev",
    "build": "npm run build:graphql && npm run build:next",
    "build:graphql": "graphql-codegen",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "ladle": "ladle serve",
    "prisma:push": "dotenv -e .env.local -- prisma db push --schema src/backend/prisma/schema.prisma",
    "prisma:generate": "dotenv -e .env.local -- prisma generate --schema src/backend/prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env.local -- prisma studio --schema src/backend/prisma/schema.prisma"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.19.4",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.13.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^13.0.0",
    "next": "12.1.5",
    "next-auth": "^4.3.4",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-resolvers": "^2.6.1",
    "@ladle/react": "^0.14.1",
    "@types/graphql-upload": "^8.0.11",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "autoprefixer": "^10.4.6",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "postcss": "^8.4.13",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
